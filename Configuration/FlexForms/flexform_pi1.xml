<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>
						Settings
					</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.enableEmailConfirm>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:enableEmailConfirm
							</label>
							<config>
								<type>check</type>
							</config>
							<onChange>reload</onChange>
						</TCEforms>
					</settings.enableEmailConfirm>
					<settings.formFieldNameIsMandatory>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:formFieldNameIsMandatory
							</label>
							<config>
								<type>check</type>
							</config>
							<onChange>reload</onChange>
						</TCEforms>
					</settings.formFieldNameIsMandatory>
					<settings.formFieldNameHidden>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:formFieldNameHidden
							</label>
							<displayCond>FIELD:settings.formFieldNameIsMandatory:=:0</displayCond>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.formFieldNameHidden>
					<settings.forceFormView>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:forceFormView
							</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.forceFormView>
					<settings.displayUnsubscribeButton>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:displayUnsubscribeButton
							</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.displayUnsubscribeButton>
					<settings.saveFolder>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:save_folder</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
								<show_thumbs>1</show_thumbs>
								<wizards>
									<suggest>
										<type>suggest</type>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</settings.saveFolder>
					<settings.userGroup>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:choose_usergroup</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>fe_groups</allowed>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
								<size>1</size>
							</config>
						</TCEforms>
					</settings.userGroup>
					<settings.confirmPage>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmPage</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<wizards>
									<suggest>
										<type>suggest</type>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</settings.confirmPage>
				</el>
			</ROOT>
		</sDEF>
		<confirmMail>
			<ROOT>
				<TCEforms>
					<sheetTitle>
						Confirmation Mail
					</sheetTitle>
					<!-- <displayCond>FIELD:settings.enableEmailConfirm:=:1</displayCond> -->
				</TCEforms>
				<type>array</type>
				<el>
					<settings.confirmMailSenderName>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmMailSenderName
							</label>
							<config>
								<type>input</type>
								<size>20</size>
								<max>30</max>
							</config>
						</TCEforms>
					</settings.confirmMailSenderName>
					<settings.confirmMailSenderEmail>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmMailSenderEmail
							</label>
							<config>
								<type>input</type>
								<size>20</size>
								<max>30</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.confirmMailSenderEmail>
					<settings.confirmMailReplyTo>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmMailReplyTo
							</label>
							<config>
								<type>input</type>
								<size>20</size>
								<max>30</max>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.confirmMailReplyTo>
					<settings.confirmMailSubject>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmMailSubject
							</label>
							<config>
								<type>input</type>
								<size>35</size>
								<max>35</max>
							</config>
						</TCEforms>
					</settings.confirmMailSubject>
					<settings.confirmMailSubscribeBody>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmMailSubscribeBody
							</label>
							<config>
								<type>text</type>
								<cols>35</cols>
								<rows>10</rows>
							</config>
						</TCEforms>
					</settings.confirmMailSubscribeBody>
					<settings.confirmMailUnsubscribeBody>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmMailUnsubscribeBody
							</label>
							<config>
								<type>text</type>
								<cols>35</cols>
								<rows>10</rows>
							</config>
						</TCEforms>
					</settings.confirmMailUnsubscribeBody>
					<settings.confirmMailSubscribeInstruction>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmMailSubscribeInstruction
							</label>
							<config>
								<type>text</type>
								<cols>35</cols>
								<rows>2</rows>
							</config>
						</TCEforms>
					</settings.confirmMailSubscribeInstruction>
					<settings.confirmMailUnsubscribeInstruction>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmMailUnsubscribeInstruction
							</label>
							<config>
								<type>text</type>
								<cols>35</cols>
								<rows>2</rows>
							</config>
						</TCEforms>
					</settings.confirmMailUnsubscribeInstruction>
					<settings.confirmMailFooter>
						<TCEforms>
							<label>
								LLL:EXT:pxa_newsletter_subscription/Resources/Private/Language/locallang_db.xlf:confirmMailFooter
							</label>
							<config>
								<type>text</type>
								<cols>35</cols>
								<rows>3</rows>
							</config>
						</TCEforms>
					</settings.confirmMailFooter>
				</el>
			</ROOT>
		</confirmMail>
	</sheets>
</T3DataStructure>